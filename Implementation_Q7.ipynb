    "#Lucky straight\n",
    "\n",
    "score = input()\n",
    "length = len(score)\n",
    "\n",
    "front = 0\n",
    "back = 0\n",
    "n = int(len(score)/2)\n",
    "for i in range(n):\n",
    "    front += int(score[i])\n",
    "    back += int(score[i+n])\n",
    "\n",
    "if front == back:\n",
    "    print(\"LUCKY\")\n",
    "else:\n",
    "    print(\"READY\")\n"
   ]
